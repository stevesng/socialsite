.gadgets-gadget-chrome {
  overflow: hidden;
  margin: 15px 0;
  border: 1px solid #616263;
  padding: 0;
  color: #333;
  font-family: arial;
  background-image: url('../../../app-ui/images/widgetFooterBackground.jpg');
  background-position: bottom;
  background-repeat: repeat-x;
}

.gadgets-gadget {
  border: 0;
  padding: 0;
  margin: 0;
}

.gadgets-gadget-title-bar {
  background-image: url('../../../app-ui/images/widgetHeaderBackground.jpg');
  background-repeat: repeat-x;
  height: 29px;
  border-bottom: 1px;
  padding: 0 .5em;
}

.gadgets-gadget-title {
  line-height: 29px;
  font-family: arial;
  font-size: 14px;
  font-weight: bold;
  color: #000000;
}

.gadgets-gadget-title-button-bar {
  float: right;
  line-height: 29px;
  font-size: smaller;
}

.gadgets-gadget-user-prefs-dialog {
  background-color: #e5ecf9;
}

.gadgets-gadget-user-prefs-dialog-action-bar {
  text-align: center;
  padding-bottom: 4px;
}

.gadgets-gadget-title-button {
}

.gadgets-gadget-content {
  margin: 0;
  padding: 0;
  border: 0;
}

.gadgets-log-entry {
}
